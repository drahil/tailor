{
    "name": "drahil/tailor",
    "description": "Enhanced Laravel Tinker with session management, code export, and smart autocomplete",
    "keywords": ["laravel", "tinker", "repl", "psysh", "session", "development"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Omar Djecevic",
            "email": "djecevic.omar@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/console": "^7.3"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
    },
    "autoload": {
        "psr-4": {
            "drahil\\Tailor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "drahil\\Tailor\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "drahil\\Tailor\\Providers\\TailorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
